<p id="notice"><%= notice %></p>

<h1>Metaclasses</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Status</th>
      <th>Niveau</th>
      <th>Listeactivites</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @metaclasses.each do |metaclass| %>
      <tr>
        <td><%= metaclass.nom %></td>
        <td><%= metaclass.status %></td>
        <td><%= metaclass.niveau %></td>
        <td><%= metaclass.listeActivites %></td>
        <td><%= link_to 'Show', metaclass %></td>
        <td><%= link_to 'Edit', edit_metaclass_path(metaclass) %></td>
        <td><%= link_to 'Destroy', metaclass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Metaclass', new_metaclass_path %>
